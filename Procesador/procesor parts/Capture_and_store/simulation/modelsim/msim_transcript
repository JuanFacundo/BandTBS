wave create -driver freeze -pattern clock -initialvalue 0 -period 110ns -dutycycle 50 -starttime 0us -endtime 1000us sim:/capture_and_store/PCLK
wave edit change_value -start 9191ps -end 804175ps -value 0 Edit:/capture_and_store/PCLK
wave create -driver freeze -pattern constant -value 0 -starttime 0us -endtime 1000us sim:/capture_and_store/RST
wave edit invert -start 96501ps -end 229764ps Edit:/capture_and_store/RST
wave create -driver freeze -pattern constant -value 0 -starttime 0us -endtime 1000us sim:/capture_and_store/HREF
