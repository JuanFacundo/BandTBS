# Loading work.vga_generator(behaviour)
wave create -driver freeze -pattern clock -initialvalue 0 -period 40ns -dutycycle 50 -starttime 0ms -endtime 32ms sim:/vga_generator/clock_25MHz
wave create -driver freeze -pattern constant -value 1 -starttime 0ms -endtime 32ms sim:/vga_generator/ena
wave create -driver freeze -pattern constant -value 0 -starttime 0ms -endtime 32ms sim:/vga_generator/rst
wave create -driver freeze -pattern random -initialvalue 0000 -period 40ns -random_type Uniform -seed 5 -range 3 0 -starttime 0ms -endtime 32ms sim:/vga_generator/data_in
add wave -position end  sim:/vga_generator/Hcount
add wave -position end  sim:/vga_generator/Hsync
add wave -position 5  sim:/vga_generator/Vsync
add wave -position end  sim:/vga_generator/Vcount
add wave -position end  sim:/vga_generator/VideoOn
add wave -position end  sim:/vga_generator/blue
wave edit invert -start 0ps -end 12587ps Edit:/vga_generator/rst
wave edit invert -start 0ps -end 49860ps Edit:/vga_generator/ena
run -all
run -all
